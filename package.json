{
  "name": "junior",
  "displayName": "junior",
  "description": "junior AI development agent",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.78.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "junior.help",
        "title": "Junior Help"
      },
      {
        "command": "junior.aichange",
        "title": "AI Change"
      }
    ],
    "configuration": {
      "title": "Junior AI",
      "properties": {
        "junior.openAIKey": {
          "type": "string",
          "default": "",
          "description": "API key for OpenAI/ChatGPT"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/diff": "^5.0.3",
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@types/uuid": "^9.0.1",
    "@types/vscode": "^1.78.0",
    "@typescript-eslint/eslint-plugin": "^5.59.1",
    "@typescript-eslint/parser": "^5.59.1",
    "@vscode/test-electron": "^2.3.0",
    "eslint": "^8.39.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "typescript": "^5.0.4",
    "uuid": "^9.0.0"
  },
  "dependencies": {
    "diff": "^5.1.0",
    "openai": "^3.2.1",
    "tiktoken": "^1.0.7"
  }
}
